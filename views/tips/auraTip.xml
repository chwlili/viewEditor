<xml >
  <depends>
    <depend src="/public/publicFrames.xml"/>
    <depend src="/public/publicIcons.xml"/>
    <depend src="/public/publicFormats.xml"/>
  </depends>
  <bitmaps>
    <bitmap id="rail" src="imgs/快捷任务区中间横条.png" />

    <bitmap id="aurabg" src="imgs/光环图标底板_锁定.png" />
    <bitmap id="aurabg2" src="imgs/光环图标底板_未锁定.png" />
    <bitmap id="dotbg" src="imgs/光环装备栏_开启.png" />
    <bitmap id="dot1" src="imgs/开点_点亮.png" />
    <bitmap id="dot2" src="imgs/开点_未开启.png" />

    <bitmap id="aura01" src="imgs/aura01.png"/>
    <bitmap id="aura02" src="imgs/aura02.png"/>
    <bitmap id="aura03" src="imgs/aura03.png"/>
    <bitmap id="aura04" src="imgs/aura04.png"/>
    <bitmap id="aura05" src="imgs/aura05.png"/>
  </bitmaps>

  
  <texts>
    <text id="光环效果">光环效果:</text>
    <text id="光环点">光环点</text>
        <text id="未解锁">未解锁</text>
            <text id="已解锁">已解锁</text>
                <text id="已装备">已装备</text>
  </texts>
  <controls>
    <box id="tip" width="253" height="170">
      <box name="aura" width="253" height="170">
        <image name="bg" bmp="mapTipsBoard" grid="50,50,50,50" left="0" top="0" width="253" height="170" />

        <image name="锁定背景" bmp="aurabg" x="20" y="10" width="68" height="66" />
        <image name="开启背景" bmp="aurabg2" x="20" y="10" width="68" height="66" />
        <image name="光环图标" x="20" y="10" width="68" height="66" />

        <label name="name" autoWidth="true" height="20"  x="100" y="20" format="宋体绿色14" html="true"/>
        <label name="解锁状态" autoWidth="true" height="20" x="100" y="42" format="宋体绿色12" html="true"/>
        <label name="lv" width="80" autoHeight="true" top="18" left="200" format="宋体绿色12" html="true"/> 


        <image bmp="rail" grid="44,0,44,0" width="253" height="3"  x="0" y="75"/>
        <label text="光环效果" format="efb55a_12" top="80" left="20" width="60" height="20"/>
        <label name="desc" width="208" autoHeight="true" top="100" left="50" format="宋体绿色12" html="true"/> 


      </box>
      <box name="dot" width="253" height="88">
        <image name="bg" bmp="mapTipsBoard" grid="50,50,50,50" left="0" top="0" width="253" height="88" />


        <image name="背景" bmp="dotbg" x="20" y="10" width="65" height="65" />
        <image name="open" bmp="dot1" x="22" y="12" width="60" height="60" />
        <image name="lock" bmp="dot2" x="22" y="12" width="60" height="60" />

        <label text="光环点" autoWidth="true" height="20" format="宋体绿色14" html="true" x="100" y="20"/>
        <label name="解锁状态" autoWidth="true" height="20" format="宋体绿色12" html="true" x="180" y="22"/>
        <label name="desc" width="208" autoHeight="true" top="42" left="100" format="提示说明文字" html="true" /> 
      </box>



    </box>
  </controls>
</xml>