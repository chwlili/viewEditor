				<checkBox name="选择按钮" right="0" bottom="0" width="16" height="16">
					<state frame="1" back="勾选按钮1"/>
					<state frame="5" back="勾选按钮2"/>
				</checkBox>
			</box>
			
			<labelButton name="确定" left="20" top="100" width="56" height="23" label="确定">
				<state frame="1" back="方角普通按钮1" backGrid="30,9,30,9" format="宋体白色12"/>
				<state frame="3" back="方角普通按钮2" backGrid="30,9,30,9" format="宋体白色12"/>
			</labelButton>
			
			<labelButton name="取消" right="20" top="100" width="56" height="23" label="取消">
				<state frame="1" back="方角普通按钮1" backGrid="30,9,30,9" format="宋体白色12"/>
				<state frame="3" back="方角普通按钮2" backGrid="30,9,30,9" format="宋体白色12"/>
			</labelButton>
		</box>
