<xml preload="0">
	<depends>
		<depend src="../public/publicFrames.xml"/>
		<depend src="../public/publicFormats.xml"/>
		<depend src="../public/publicButtons.xml"/>
	</depends>
	<swfs>
		<swf id="emote" src="swfs/ems.swf" size="200759"/>
		<swf id="titles" src="swfs/titles.swf" size="200759"/>
	</swfs>
	
	<texts>
		<text id="选择道具">选择道具</text>
		<text id="确定">确定</text>
		<text id="取消">取消</text>
	</texts>

	<controls>
		<view id="view" emsnum="71" widthnum="11" x="260" y="570" >
			<box id="表情列表.box" x="200" y="400" visible="false">
				<image bmp="表情框背景" grid="5,5,5,5" left="0" right="0" top="0" bottom="0"/>
