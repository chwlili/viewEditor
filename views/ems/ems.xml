			</box>
		</view>

		<box id="forbidWin" middle="0" center="0" width="174" height="145">
			
			<image bmp="1级底板2层.bmp" left="0" right="0" top="0" bottom="0" grid="50,50,50,50" />
			
			<label center="0" width="60" top="20" height="20" text="选择道具" format="宋体白色12居中"/>
			
			<box name="普通禁言容器" left="30" y="40" width="47" height="47">
				<bitmap name="图标字段" x="5" y="5" width="45" height="45"/>
				<label name="数量字段" left="5" bottom="5" width="37" height="15" format="宋体白色12"/>
				<checkBox name="选择按钮" right="0" bottom="0" width="16" height="16">
					<state frame="1" back="勾选按钮1"/>
					<state frame="5" back="勾选按钮2"/>
				</checkBox>
			</box>
			
			<box name="永久禁言容器" right="30" y="40" width="47" height="47">
				<bitmap name="图标字段" x="5" y="5" width="45" height="45"/>
