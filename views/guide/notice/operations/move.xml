<xml>
	<depends>
		<depend src="skillkeyboard.xml"/>
	</depends>
	<bitmaps>
		<bitmap id="背景" src="背景.png"/>
		<bitmap id="操作指南" src="操作指南.png"/>
		
		<bitmap id="Key" src="按钮-小.png"/>

		<bitmap id="A" src="A键.png"/>
		<bitmap id="W" src="W键.png"/>
		<bitmap id="S" src="S键.png"/>
		<bitmap id="D" src="D键.png"/>

		<bitmap id="U" src="U键.png"/>
		<bitmap id="I" src="I键.png"/>
		<bitmap id="O" src="O键.png"/>
		<bitmap id="H" src="H键.png"/>
		<bitmap id="J" src="J键.png"/>
		<bitmap id="K" src="K键.png"/>
		<bitmap id="L" src="L键.png"/>

		<bitmap id="标题名称移动" src="标题文字-移动.png"/>
		<bitmap id="标题名称奔跑" src="标题文字-奔跑.png"/>

		<bitmap id="def_key" src="按钮01.png"/>
		<bitmap id="def_W" src="按钮01W.png"/>
		<bitmap id="def_S" src="按钮01S.png"/>

		<bitmap id="up_key" src="按钮01-红.png"/>
		<bitmap id="down_key" src="按钮02-红.png"/>
		
		<bitmap id="up_A" src="按钮01-红A.png"/>
		<bitmap id="down_A" src="按钮02-红A.png"/>

		<bitmap id="up_D" src="按钮01-红D.png"/>
		<bitmap id="down_D" src="按钮02-红D.png"/>

		<bitmap id="左手" src="手指.png"/>		
		<bitmap id="右手" src="手指-右手.png"/>
	</bitmaps>

	<controls>

		<box id="引导框" x="321" y="40" width="226" height="82">
			<image bmp="背景" width="226" height="82" grid="10,10,10,10"/>
			<image bmp="操作指南" width="64" height="22" center="0" top="-8"/>


			<box name="动画面板" x="0" y="88" width="226" height="1">
				<image name="moveTitle" bmp="标题名称移动" width="181" height="28" center="0" top="0" visible="false" />
				<image name="runTitle" bmp="标题名称奔跑" width="181" height="28" center="0" top="0" visible="false"/>

				<box name="按键动画" width="73" height="33" center="-10" top="35">
					<box name="a_up" x="0" y="35" width="33" height="33">
						<bitmap bitmap="up_key" width="33" height="33"/>
						<bitmap bitmap="up_A"   width="19" height="18" x="7" y="5"/>
					</box>
					<box name="a_down" x="0" y="35" width="33" height="33">
						<bitmap bitmap="down_key" width="33" height="33"/>
						<bitmap bitmap="down_A"   width="19" height="18" x="7" y="8"/>
					</box>
					
					<box name="d_up" x="70" y="35" width="33" height="33">
						<bitmap bitmap="up_key" width="33" height="33"/>
						<bitmap bitmap="up_D"   width="19" height="18" x="7" y="5"/>
					</box>
					<box name="d_down" x="70" y="35" width="33" height="33">
						<bitmap bitmap="down_key" width="33" height="33"/>
						<bitmap bitmap="down_D"   width="19" height="18" x="7" y="8"/>
					</box>
					
					<box x="35" y="0" width="33" height="33">
						<bitmap bitmap="def_key" width="33" height="33"/>
						<bitmap bitmap="def_W"   width="15" height="12" x="7" y="8"/>
					</box>
					<box x="35" y="35" width="33" height="33">
						<bitmap bitmap="def_key" width="33" height="33"/>
						<bitmap bitmap="def_S"   width="15" height="12" x="7" y="8"/>
					</box>
					<bitmap name="handler_left" bitmap="左手" width="68" height="94" x="-47" y="21"/>
				</box>
			</box>
		</box>
		
	</controls>

</xml>